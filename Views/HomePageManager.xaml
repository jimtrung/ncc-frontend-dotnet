<Window x:Class="Theater_Management_FE.Views.HomePageManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DockPanel>
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal"
                    HorizontalAlignment="Left">
            <Button x:Name="movieButton" Content="Movie"/>
            <Button x:Name="auditoriumButton" Content="Auditorium"/>
            <Button x:Name="profileButton" Content="Profile"/>
            <Button x:Name="logOutButton" Content="Log Out"/>
        </StackPanel>
        <Separator DockPanel.Dock="Top"/>
        <Grid x:Name="contentGrid">
        </Grid>
    </DockPanel>
</Window>
