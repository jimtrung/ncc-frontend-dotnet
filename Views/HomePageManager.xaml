                <Button x:Name="profileButton" Content="Profile" Style="{StaticResource button-primary}" Margin="0,0,10,0"/>
                <Button x:Name="logOutButton" Content="Log Out" Style="{StaticResource button-danger}"/>
            </StackPanel>
        </Border>
        <Grid x:Name="contentGrid" Margin="20"/>
    </DockPanel>
</Window>
